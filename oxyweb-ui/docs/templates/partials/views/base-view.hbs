			<script src="{{assets}}vendor/holderjs/holder.js"></script>
			<style type="text/css">
				img.placeholder {
					background-color: #EEEEEE;
					-webkit-box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.3);
					box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.3);
				}
			</style>
			<!-- </{{package.name}}:placeholder> -->
		{{/block}}
	</head>
	<body class="{{#block "layout"}}base{{/block}}-layout has-tooltips"{{#block "body-attrs"}}{{/block}}>
		{{#block "base"}}{{/block}}
